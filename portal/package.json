{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",

    "postinstall": "prisma generate --schema prisma/schema.prisma",

    "build": "prisma generate --schema prisma/schema.prisma && next build",
    "start": "next start",

    "build:local": "dotenv -e .env.local -- prisma generate --schema prisma/schema.prisma && dotenv -e .env.local -- next build",
    "start:local": "dotenv -e .env.local -- next start",

    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:enum-drift": "git grep -n 'status:\\s*\"(active|planned|frozen|parked)\"' -- portal && exit 1 || exit 0",

    "prisma:format": "prisma format",
    "prisma:format:local": "dotenv -e .env.local -- prisma format",

    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:local": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:status:local": "dotenv -e .env.local -- prisma migrate status",

    "prisma:studio": "prisma studio",
    "prisma:studio:local": "dotenv -e .env.local -- prisma studio",

    "backfill:workpacket-sot": "dotenv -e .env.local -- tsx scripts/backfill-sotevent-workpacketid.ts",

    "seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",

    "ingest:event": "dotenv -e .env.local -- tsx scripts/ingest-sot-event.ts",
    "audit:q4-2025": "dotenv -e .env.local -- tsx scripts/run-q4-audit.ts",

    "emit:offbook-v0": "dotenv -e .env.local -- tsx scripts/emit-offbook-v0-deployed.ts",
    "emit:autopilot-v0": "dotenv -e .env.local -- tsx scripts/emit-autopilot-v0-init.ts",
    "emit:autopilot-demo": "dotenv -e .env.local -- tsx scripts/emit-autopilot-demo.ts",

    "sot:post": "dotenv -e .env.local -- tsx scripts/jai-sot-post.ts",

    "jai:agent:repos": "dotenv -e .env.local -- tsx scripts/jai-agent-repo-status.ts",
    "jai:wave:plan": "dotenv -e .env.local -- tsx scripts/jai-agent-plan-wave.ts",
    "jai:wave:apply": "dotenv -e .env.local -- tsx scripts/jai-wave-apply.ts",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/adapter-pg": "7.0.1",
    "@prisma/client": "^7.1.0",
    "@prisma/client-runtime-utils": "^7.1.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^7.4.2",
    "js-yaml": "^4.1.1",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "react": "^19",
    "react-dom": "^19",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.22",
    "baseline-browser-mapping": "^2.9.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8.5.6",
    "prisma": "^7.1.0",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
